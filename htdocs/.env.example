# Name of the app
APP_NAME="My Business"

# Full url to image, or leave empty for no logo
APP_LOGO="" 

# The url where the app is installed
APP_URL=http://localhost

# Public e-mail address visible on site (contact)
APP_CONTACT_EMAIL="hi@example.com"

# Environment, "production" is recommended
APP_ENV="production"

# Key is automatically generated during installation
APP_KEY=

# Whether to show full error messages (false recommended for production, true for testing) 
APP_DEBUG=true

# Defaults
DEFAULT_LANGUAGE="en"
DEFAULT_LOCALE="en_US"
DEFAULT_TIMEZONE="UTC"
DEFAULT_CURRENCY="USD"

# Social links in footer
# Enter the full url to your social account
SOCIAL_TWITTER=""
SOCIAL_YOUTUBE=""
SOCIAL_FACEBOOK=""
SOCIAL_INSTAGRAM=""
SOCIAL_MEDIUM=""
SOCIAL_SNAPCHAT=""
SOCIAL_LINKEDIN=""
SOCIAL_GITHUB=""

# Database
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=root
DB_PASSWORD=

# Sender name and e-mail address 
MAIL_FROM_NAME="My Business"
MAIL_FROM_ADDRESS="noreply@example.com"

# Most hosting providers support the "mail" driver,
# so you can leave the settings below. However, for a better performance 
# and spam rating you can consider another driver. 
# https://laravel.com/docs/6.x/mail 

MAIL_DRIVER=mail
MAIL_HOST=smtp.mailgun.org
MAIL_PORT=587
MAIL_ENCRYPTION=tls
MAIL_USERNAME=null
MAIL_PASSWORD=null

# Google Analytics code
# UA-xxxxxxxxx-x
CONFIG_GOOGLE_ANALYTICS=""

LOG_CHANNEL=stack
BROADCAST_DRIVER=log
CACHE_DRIVER=file
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1

MIX_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
MIX_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"
